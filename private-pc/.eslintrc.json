{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "rules": {
    "no-console": "off",
    "eqeqeq": 1,
    "indent": [
      2,
      2
    ]
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "globals": {
    "Global_config": true,
    "SignaturePad": true,
    "AMap": true,
    "AMapUI": false,
    "echarts": true
  }
}
